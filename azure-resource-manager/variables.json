{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "inputValue": {
            "type": "string",
            "defaultValue": "deployment parameter"
        }
    },
    "variables": {
        "var1": "myVariable",
        "var2": "[concat(variables('var1'), '-next') ]",
        "var3": "[concat(parameters('inputValue'), '-fromparam')]",
        "var4": [
            1,
            2,
            3,
            4
        ],
        "var5": {
            "property1": "value1",
            "property2": "value2"
        },
        "var6": {
            "copy": [
                {
                    "name": "disks",
                    "count": 5,
                    "input": {
                        "name": "[concat('myDataDisk', copyIndex('disks', 1))]",
                        "diskSizeGB": "1",
                        "diskIndex": "[copyIndex('disks')]"
                    }
                },
                {
                    "name": "diskNames",
                    "count": 5,
                    "input": "[concat('myDataDisk', copyIndex('diskNames', 1))]"
                }
            ]
        },
        "copy": [
            {
                "name": "var7",
                "count": 5,
                "input": {
                    "name": "[concat('oneDataDisk', copyIndex('var7', 1))]",
                    "diskSizeGB": "1",
                    "diskIndex": "[copyIndex('var7')]"
                }
            },
            {
                "name": "var8",
                "count": 3,
                "input": {
                    "name": "[concat('twoDataDisk', copyIndex('var8', 1))]",
                    "diskSizeGB": "1",
                    "diskIndex": "[copyIndex('var8')]"
                }
            },
            {
                "name": "var9",
                "count": 4,
                "input": "[concat('stringValue', copyIndex('var9'))]"
            },
            {
                "name": "var10",
                "count": 4,
                "input": "[copyIndex('var10')]"
            }
        ]
    },
    "resources": [],
    "outputs": {
        "exampleOutput1": {
            "value": "[variables('var1')]",
            "type": "string"
        },
        "exampleOutput2": {
            "value": "[variables('var2')]",
            "type": "string"
        },
        "exampleOutput3": {
            "value": "[variables('var3')]",
            "type": "string"
        },
        "exampleOutput4": {
            "value": "[variables('var4')]",
            "type": "array"
        },
        "exampleOutput5": {
            "type": "object",
            "value": "[variables('var5')]"
        },
        "exampleOutput6": {
            "type": "object",
            "value": "[variables('var6')]"
        },
        "exampleOutput7": {
            "type": "array",
            "value": "[variables('var7')]"
        },
        "exampleOutput8": {
            "type": "array",
            "value": "[variables('var8')]"
        },
        "exampleOutput9": {
            "type": "array",
            "value": "[variables('var9')]"
        },
        "exampleOutput10": {
            "type": "array",
            "value": "[variables('var10')]"
        }
    }
}